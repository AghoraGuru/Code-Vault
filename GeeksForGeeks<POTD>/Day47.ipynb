    "class Solution:\n",
    "    def solve(self, a : int, b : int) -> int:\n",
    "        # code here\n",
    "        count = 0\n",
    "        while a!=b:\n",
    "            if a > b:\n",
    "                a = a &b\n",
    "            else:\n",
    "                b = b&a\n",
    "            count += 1\n",
    "        return count"
   ]
